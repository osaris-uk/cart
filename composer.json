{
    "name": "osaris-uk/cart",
    "description": "Shopping Cart library for Laravel 6 that uses Database instead of Sessions",
    "keywords": ["laravel", "cart", "database", "laravel6", "shopping-cart", "ecommerce"],
    "homepage": "https://github.com/osaris-uk/cart",
    "license": "MIT",
    "authors": [
        {
            "name": "osaris-uk",
            "email": "john.dent@osaris.co.uk"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "~6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-0": {
            "OsarisUk\\Cart": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra":{
      "laravel":{
         "providers":[
            "OsarisUk\\Cart\\CartServiceProvider"
         ]
      }
   }
}
